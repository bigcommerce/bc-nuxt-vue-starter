{
  "name": "bc-nuxt-vue-starter",
  "version": "1.0.0",
  "description": "BC + Nuxt + Storefront UI Starter",
  "author": "Nate Stewart",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "dev-debug": "node --inspect node_modules/.bin/nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "set-heroku-env": "sh ./set-heroku-env.sh",
    "create-storefront-token": "node ./utils/script/create-storefront-token createStoreFrontToken",
    "create-storefront-channel": "node ./utils/script/create-storefront-channel createChannel",
    "create-storefront-site": "node ./utils/script/create-storefront-site createSite",
    "create-storefront-route": "node ./utils/script/create-storefront-route createRoute",
    "deploy": "npm run generate && vercel"
  },
  "dependencies": {
    "@bigcommerce/checkout-sdk": "^1.206.0",
    "@nuxtjs/axios": "^5.12.4",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/pwa": "^3.3.3",
    "@nuxtjs/sentry": "^5.1.3",
    "@nuxtjs/toast": "^3.3.1",
    "@storefront-ui/vue": "^0.11.0",
    "core-js": "^3.8.1",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "nuxt": "^2.14.12",
    "set-cookie-parser": "^2.4.8",
    "uuid": "^8.3.2",
    "vue-append": "^2.0.1",
    "vue-country-region-select": "^2.0.14",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^27.2.0",
    "lodash": "^4.17.21",
    "make-runnable": "^1.3.10",
    "prettier": "^2.2.1",
    "supertest": "^6.1.6",
    "vue-jest": "^3.0.7"
  }
}
